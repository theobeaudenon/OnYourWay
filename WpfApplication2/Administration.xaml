<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI" x:Class="WpfApplication2.Administration" 
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="1300">

    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab SelectedSource="Pages/adminpage.xaml" Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Employees" Source="Employees/mainS.xaml" />
                <mui:Link DisplayName="Carte" Source="Carte/AccC.xaml" />
                <mui:Link DisplayName="Produits" Source="Produits/mainP.xaml" />
                <mui:Link DisplayName="Categories" Source="cat/mainC.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
    
    
    

</UserControl>
